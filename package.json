{
  "name": "modern-web-app",
  "version": "1.0.0",
  "description": "A modern web application using React, Apollo GraphQL, TypeORM, Postgres, and Express.js.",
  "repository": "https://github.com/meebix/modern-web-app",
  "author": "Michael Donahue <mtdonahue1@gmail.com>",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "pre-commit": "lint-staged",
    "build": "./scripts/build.sh",
    "build:analyze": "ANALYZE=true yarn build",
    "start": "NODE_ENV=production node .build/src/server/index.js",
    "dev": "nodemon",
    "lint:ts": "eslint src --ext '.ts,.tsx' --quiet",
    "lint:css": "stylelint src/**/*.{css,scss} --quiet",
    "lint": "yarn lint:ts && yarn lint:css",
    "prettier:fix": "prettier \"**/*.{ts,tsx}\" --write",
    "ts": "tsc --noEmit & tsc -p src/server/tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migration:create": "typeorm migration:create -n untitled",
    "db:migration:run": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:run",
    "db:migration:revert": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:revert",
    "db:migration:show": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:show",
    "db:seed:create": "typeorm migration:create -n untitled -d src/server/seeders",
    "db:seed:run": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:run -c seed",
    "db:seed:reset": "babel-node --extensions '.ts' ./node_modules/typeorm/cli.js migration:revert -c seed"
  },
